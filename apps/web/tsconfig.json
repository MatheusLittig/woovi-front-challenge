{
  "extends": "@woovi/presets/json/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@res/*": [
        "../../packages/stores/src/*"
      ],
      "@woovi/stores/react/*": [
        "../../packages/stores/src/adapters/react/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
